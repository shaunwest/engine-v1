    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "history": "^2.0.0-rc2",
    "isomorphic-fetch": "^2.2.0",
    "koa": "^1.1.2",
    "koa-body": "^1.4.0",
    "koa-static": "^2.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-devtools": "^3.0.1",
    "redux-logger": "^2.3.2",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
